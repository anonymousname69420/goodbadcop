l av=al*ap;local aw=al*aq;local ax=al*ar;local ay=am*aq;local az=am*ar;local aA=an*ar;return CFrame.new(ai,aj,ak,1-(ay+aA),aw-au,ax+at,aw+au,1-(av+aA),az-as,ax-at,az+as,1-(av+ay))end;function QuaternionSlerp(a0,a1,aB)local aC=a0[1]*a1[1]+a0[2]*a1[2]+a0[3]*a1[3]+a0[4]*a1[4]local aD,aE;if aC>=0.0001 then if 1-aC>0.0001 then local aF=ACOS(aC)local aG=1/SIN(aF)aD=SIN((1-aB)*aF)*aG;aE=SIN(aB*aF)*aG else aD=1-aB;aE=aB end else if 1+aC>0.0001 then local aF=ACOS(-aC)local aG=1/SIN(aF)aD=SIN((aB-1)*aF)*aG;aE=SIN(aB*aF)*aG else aD=aB-1;aE=aB end end;return a0[1]*aD+a1[1]*aE,a0[2]*aD+a1[2]*aE,a0[3]*aD+a1[3]*aE,a0[4]*aD+a1[4]*aE end;function Clerp(a0,a1,aB)local aH={QuaternionFromCFrame(a0)}local aI={QuaternionFromCFrame(a1)}local aJ,aK,aL=a0.x,a0.y,a0.z;local aM,aN,aO=a1.x,a1.y,a1.z;local aP=1-aB;return QuaternionToCFrame(aP*aJ+aB*aM,aP*aK+aB*aN,aP*aL+aB*aO,QuaternionSlerp(aH,aI,aB))end;function CreateFrame(K,U,aQ,E,X,aR,aS,W)local frame=IT("Frame")frame.BackgroundTransparency=U;frame.BorderSizePixel=aQ;frame.Position=E;frame.Size=X;frame.BackgroundColor3=aR;frame.BorderColor3=aS;frame.Name=W;frame.Parent=K;return frame end;function CreateLabel(K,aT,aU,aV,aW,U,aQ,aX,W)local aY=IT("TextLabel")aY.BackgroundTransparency=1;aY.Size=UD2(1,0,1,0)aY.Position=UD2(0,0,0,0)aY.TextColor3=aU;aY.TextStrokeTransparency=aX;aY.TextTransparency=U;aY.FontSize=aV;aY.Font=aW;aY.BorderSizePixel=aQ;aY.TextScaled=false;aY.Text=aT;aY.Name=W;aY.Parent=K;return aY end;function NoOutlines(aZ)aZ.TopSurface,aZ.BottomSurface,aZ.LeftSurface,aZ.RightSurface,aZ.FrontSurface,aZ.BackSurface=10,10,10,10,10,10 end;function CreateWeldOrSnapOrMotor(a_,K,b0,b1,b2,b3)local b4=IT(a_)b4.Part0=b0;b4.Part1=b1;b4.C0=b2;b4.C1=b3;b4.Parent=K;return b4 end;local b5=IT("Sound")function CreateSound(b6,K,b7,b8,b9)local ba=nil;coroutine.resume(coroutine.create(function()ba=b5:Clone()ba.Parent=K;ba.Volume=b7;ba.Pitch=b8;ba.SoundId="http://www.roblox.com/asset/?id="..b6;ba:play()if b9==true then ba.Looped=true else repeat wait(1)un